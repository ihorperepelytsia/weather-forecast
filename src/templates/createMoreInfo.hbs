{{#each this}}
<div class="more-info-card" data-day="{{this.date}}">
    <p class="more-info-card__time">{{this.timeDt}}</p>
    <div class="more-info-card__sky"><img src="http://openweathermap.org/img/wn/{{this.icon}}@2x.png"></div>
    <p class="more-info-card__temperature">{{this.temp}}&#176</p>
    <div class="more-info-card__details">
        <img src="data:image/png;base64,"><span>{{this.pressure}} mm</span>
    </div>
    <div class="more-info-card__details">
        <img src="data:image/png;base64,"><span>{{this.humidity}}%</span>
    </div>
    <div class="more-info-card__details">
        <img src="data:image/png;base64,"><span>{{this.speed}} m/s</span>
    </div>
</div>
{{/each}}